/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way (in=in[0..7], out=ls);
    Or8Way (in=in[8..15], out=ms);
    Or (a=ls, b=ms, out=out);
}